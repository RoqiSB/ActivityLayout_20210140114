<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".frame_layout">

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:srcCompat="@drawable/backg" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:srcCompat="@drawable/genshin_logo"
        android:layout_gravity="top|left"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="20dp"
        />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Game Wibu"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:layout_gravity="top|center"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="20dp"

        />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Ini adalah game orang yang bau bawang"
        android:textStyle="normal"
        android:textColor="@color/black"
        android:layout_gravity="top|center"
        android:layout_marginTop="45dp"
        android:layout_marginLeft="50dp"
        />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_gravity="center|right"
        android:layout_marginRight="40dp"
        app:srcCompat="@drawable/baseline_chevron_right_24" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_gravity="center|left"
        android:layout_marginLeft="40dp"
        app:srcCompat="@drawable/baseline_chevron_left_24" />
</FrameLayout>